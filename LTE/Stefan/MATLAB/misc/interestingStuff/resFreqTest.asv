
L = 2;
C = 2;
G = 0.1;
omega_r = 1 / sqrt(L * C);
omega = 0 : 0.01 : 2;
alpha = 0 : 0.001 : 0.3;
V = zeros(length(omega), length(alpha));
for omegaCnt = 1 : length(omega)
  for alphaCnt = 1 : length(alpha)
    s0 = -alpha(alphaCnt) + omega(omegaCnt) * j;
    V(omegaCnt, alphaCnt) = -1 / C * s0 ./ (s0.^2 + s0 * G / C + omega_r^2);
  end
end
surf(omega,  abs(V));




